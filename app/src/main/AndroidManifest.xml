<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ebukom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".arch.ui.chooseclass.ChooseClassEmptyActivity"></activity>
        <activity android:name=".arch.ui.register.school.RegisterSchoolActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".arch.ui.register.parent.RegisterParentActivity" />
        <activity android:name=".arch.ui.forgotpassword.VerificationActivity" />
        <activity android:name=".arch.ui.forgotpassword.SendCodeActivity" />
        <activity android:name=".arch.ui.login.LoginActivity" />
        <activity android:name=".arch.ui.splash.SplashActivity" />
        <activity android:name=".arch.ui.main.MainActivity" />
    </application>

</manifest>